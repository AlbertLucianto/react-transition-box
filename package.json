{
  "name": "transitioning-container",
  "version": "1.0.0",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:AlbertLucianto/transitioning-container.git",
  "author": "AlbertLucianto <albertlucianto@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "transition",
    "animation"
  ],
  "scripts": {
    "unit": "cross-env NODE_ENV=test jest --config __test__/jest.conf.js --coverage",
    "test": "npm run unit",
    "dev": "cross-env NODE_ENV=development TARGET=desktop webpack-dev-server --inline --progress --hot --mode development --config build/webpack.config.js",
    "build:web": "rimraf dist/web && cross-env NODE_ENV=production webpack --inline --progress --mode production --config build/webpack.config.js",
    "build:browser": "rollup --config build/rollup.browser.js",
    "build:dev": "rollup --config build/rollup.config.js",
    "build": "npm run build:dev && npm run build:browser && npm run build:web",
    "coverage": "codecov -f __test__/coverage/coverage-final.json"
  },
  "devDependencies": {
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.4.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  }
}
