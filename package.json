  "name": "react-transitioning-container",
  "version": "1.0.2",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:AlbertLucianto/react-transitioning-container.git",
  "author": "AlbertLucianto <albertlucianto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbertLucianto/react-transitioning-container/issues"
  },
  "homepage": "https://github.com/AlbertLucianto/react-transitioning-container",
  "keywords": [
    "react",
    "transition",
    "animation"
  ],
  "scripts": {
    "unit": "cross-env NODE_ENV=test jest --config __test__/jest.conf.js --coverage",
    "test": "npm run unit",
    "test:clean": "cross-env NODE_ENV=test jest --config __test__/jest.conf.js --coverage --updateSnapshot",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-build-config.json\" NODE_ENV=development TARGET=desktop webpack-dev-server --inline --progress --hot --mode development --config build/webpack.config.ts",
    "build:web": "rimraf dist/web && cross-env TS_NODE_PROJECT=\"tsconfig-for-build-config.json\" NODE_ENV=production webpack --inline --progress --mode production --config build/webpack.config.ts",
    "build:browser": "cross-env TS_NODE_PROJECT=\"tsconfig-for-build-config.json\" rollup --config build/rollup.browser.ts",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-build-config.json\" rollup --config build/rollup.config.ts",
    "build": "npm run build:dev && npm run build:browser && npm run build:web",
    "coverage": "codecov -f __test__/coverage/coverage-final.json"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/react-transition-group": "^2.0.16",
    "@types/rollup": "^0.54.0",
    "@types/rollup-plugin-commonjs": "^9.2.0",
    "@types/rollup-plugin-node-resolve": "^4.0.0",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-server": "^3.1.4",
    "autoprefixer": "^9.4.10",
    "classnames": "^2.2.6",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.4.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^6.0.0",
    "react-transition-group": "^2.6.1",
    "react-transitioning-container": "^0.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
